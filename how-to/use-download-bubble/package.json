{
	"name": "use-download-bubble",
	"version": "40.104.2",
	"description": "A basic window with a button that triggers a download (Chromium download bubble UI).",
	"main": "index.js",
	"scripts": {
		"kill": "node ./scripts/kill.mjs",
		"client": "node ./scripts/launch.mjs",
		"build-client": "webpack build --config ./client/webpack.config.js --mode=development",
		"build": "npm run build-client",
		"start": "npx --yes http-server ./public -p 5050 -c-1",
		"setup": "cd ../../ && npm install && cd how-to/use-download-bubble && npm run build"
	},
	"author": "nick.newman@openfin.co",
	"license": "SEE LICENSE IN LICENSE.MD",
	"devDependencies": {
		"@openfin/core": "43.101.2",
		"@openfin/node-adapter": "43.101.2",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	}
}
