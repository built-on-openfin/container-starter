{
  "runtime": {
    "arguments": "--v=1 --inspect --security-realm=PROD-MAIN",
    "version": "stable"
  },
  "shortcut": {
    "company": "OpenFin",
    "description": "PROD Main Platform",
    "icon": "http://localhost:8080/favicon.ico",
    "name": "PROD Main Platform"
  },
  "platform": {
    "uuid": "prod-main-platform",
    "autoShow": false,
    "permissions": {
      "System": {
        "launchExternalProcess": true
      }
    }
  },
  "snapshot": {
    "windows": [
      {
        "layout": {
          "content": [
            {
              "type": "stack",
              "id": "no-drop-target",
              "content": [
                {
                  "type": "component",
                  "componentName": "view",
                  "componentState": {
                    "processAffinity": "ps_1",
                    "url": "http://localhost:8080/html/view-app.html"
                  }
                }
              ]
            }
          ]
        }
      }
    ]
  }
}