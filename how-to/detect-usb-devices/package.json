	"license": "SEE LICENSE IN LICENSE.MD",
	"devDependencies": {
		"@openfin/core": "38.83.79",
		"@openfin/node-adapter": "38.83.79",
		"@types/w3c-web-usb": "^1.0.10",
		"ts-loader": "^9.5.1",
		"typescript": "^5.4.5",
		"webpack": "^5.91.0",
		"webpack-cli": "^5.1.4"
	}
}
