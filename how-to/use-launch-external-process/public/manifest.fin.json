{
  "runtime": {
    "version": "27.104.71.29"
  },
  "startup_app": {
    "name": "launch-app-asset",
    "url": "http://localhost:5050/app.html",
    "uuid": "launch-app-asset",
    "icon": "http://localhost:5050/common/images/icon-blue.png",
    "autoShow": true,
    "saveWindowState": true,
    "permissions": {
      "System": {
        "launchExternalProcess": true,
        "terminateExternalProcess": true,
        "downloadAsset": true
      }
    }
  },
  "appAssets": [
    {
      "src": "http://localhost:5050/assets/openfin-dotnet-example.zip",
      "version": "3.1",
      "alias": "of-dotnet-example",
      "target": "DotNetCore.exe",
      "mandatory": true
    }
  ],
  "shortcut": {
    "company": "OpenFin",
    "description": "Launch App Asset",
    "icon": "http://localhost:5050/common/images/icon-blue.png",
    "name": "Launch App Asset"
  }
}