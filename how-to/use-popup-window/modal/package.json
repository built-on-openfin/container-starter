{
	"name": "use-popup-window-modal",
	"version": "0.0.1",
	"description": "A demonstration of a modal popup window.",
	"main": "index.js",
	"scripts": {
		"build": "npm run build-server & npm run build-client",
		"build-server": "tsc --project ./server",
		"build-client": "webpack build --config ./client/webpack.config.js --mode=development",
		"build-client-prod": "webpack build --config ./client/webpack.config.js --mode=production",
		"start": "npm run server",
		"client": "npm --prefix ../../common/ run client",
		"server": "node ./server/build/app.js",
		"kill": "npm --prefix ../../common/ run kill",
		"setup": "cd ../../../ && npm install && cd how-to/use-popup-window/modal && npm run build",
		"validate": "npm --prefix ../../../ run validate"
	},
	"author": "connor.mccafferty@openfin.co",
	"license": "SEE LICENSE IN LICENSE.MD",
	"devDependencies": {
		"@openfin/core": "^27.70.8",
		"@types/node": "^18.7.6",
		"express": "^4.17.2",
		"source-map-loader": "^3.0.1",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.4",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.9.1"
	}
}
