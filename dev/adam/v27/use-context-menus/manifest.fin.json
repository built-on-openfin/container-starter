		"autoShow": false,
		"providerUrl": "https://built-on-openfin.github.io/container-starter/dev/adam/v27/use-context-menus/html/platform-provider.html",
		"waitForPageLoad": false,
		"defaultWindowOptions": {
			"waitForPageLoad": false,
			"url": "https://built-on-openfin.github.io/container-starter/dev/adam/v27/use-context-menus/html/default-platform-window.html",
