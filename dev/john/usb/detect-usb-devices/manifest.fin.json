{
	"runtime": {
		"arguments": "--v=0",
		"version": "40.130.104.2"
	},
	"platform": {
		"uuid": "how-to-detect-usb-devices",
		"autoShow": false,
		"icon": "https://built-on-openfin.github.io/container-starter/dev/john/usb/detect-usb-devices/favicon.ico",
		"permissions": {
			"webAPIs": ["audio", "video", "hid", "usb"]
		},
		"defaultViewOptions": {
			"permissions": {
				"webAPIs": ["audio", "video", "hid", "usb"]
			}
		},
		"defaultWindowOptions": {
			"permissions": {
				"webAPIs": ["audio", "video", "hid", "usb"]
			}
		},
		"defaultDomainSettings": {
			"rules": [
				{
					"match": ["<all_urls>"],
					"options": {
						"downloadSettings": {
							"rules": [
								{
									"match": ["<all_urls>"],
									"behavior": "no-prompt"
								}
							]
						}
					}
				}
			]
		}
	},
	"snapshot": {
		"windows": [
			{
				"layout": {
					"content": [
						{
							"type": "stack",
							"id": "no-drop-target",
							"content": [
								{
									"type": "component",
									"componentName": "view",
									"componentState": {
										"processAffinity": "ps_1",
										"url": "https://built-on-openfin.github.io/container-starter/dev/john/usb/detect-usb-devices/html/app.html",
										"permissions": {
											"webAPIs": ["hid", "usb"]
										}
									}
								}
							]
						}
					]
				}
			}
		]
	}
}
